{
    "env": {
        "es": {
            "presets": [
                ["@babel/preset-env", {
                    "modules": false
                }]
            ]
        },
        "development": {
            "presets": [
                ["@babel/preset-env", { "targets": { "node": "0.8" } }]
            ],
            "plugins": [
              [
                "@babel/plugin-proposal-class-properties",
                {
                  "loose": true
                }
              ],
                [
                    "add-module-exports",
                    {
                        "addDefaultProperty": true
                    }
                ]
            ]
        }
    }
}